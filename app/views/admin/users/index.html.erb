<main>
  <div class="container">
    <div class="row">
    </div>
    <div class="row">
      <div class="col s12 m9 l10">
        <div class="tables" class="section scrollspy">
          <p class="caption"></p>
          <h2 class="header"></h2>
          <div class="row">
            <div class="col s12">
              <p></p>
              <table>
                <thead>
                  <tr>
                    <th data-field="id">ID</th>
                    <th data-field="name">Name</th>
                    <th data-field="email">Email</th>
                    <th data-field="sign_in_url">Sign in URL</th>
                  </tr>
                </thead>
                <tbody>
                  <% @users.each do |user| %>
                    <tr>
                      <td><%= user.id %></td>
                      <td><%= user.name %></td>
                      <td><%= user.email %></td>
                      <td><%= user.sign_in_url %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
        </div>
      </div>
    </div>
  </div>
</main>
